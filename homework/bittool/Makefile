# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  bittool_config = debug
endif
ifeq ($(config),release)
  bittool_config = release
endif

PROJECTS := bittool

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

bittool:
ifneq (,$(bittool_config))
	@echo "==== Building bittool ($(bittool_config)) ===="
	@${MAKE} --no-print-directory -C . -f bittool.make config=$(bittool_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f bittool.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   bittool"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"